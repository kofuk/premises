# -*- shell-script -*-

# Run web server debug mode. (bool)
PREMISES_DEBUG=false

# ConoHa username. (string)
PREMISES_CONOHA_USERNAME=user

# ConoHa access token (not real password to login to ConoHa's control panel). (string)
PREMISES_CONOHA_PASSWORD=password

# ConoHa tenant ID. (string)
PREMISES_CONOHA_TENANT_ID=tenantId

# ConoHa endpoint for  Identity service. (string)
PREMISES_CONOHA_IDENTITY_SERVICE=http://host.docker.internal:8010/identity/v3

# ConoHa endpoint for Compute service. (string)
PREMISES_CONOHA_COMPUTE_SERVICE=http://host.docker.internal:8010/compute/v2.1

# ConoHa endpoint for Network service. (string)
PREMISES_CONOHA_NETWORK_SERVICE=http://host.docker.internal:8010/network

# ConoHa endpoint for Volume service. (string)
PREMISES_CONOHA_VOLUME_SERVICE=http://host.docker.internal:8010/volume/v3

# Name tag used for premises-related resources
PREMISES_CONOHA_NAME_TAG=mc-premises

# S3 endpoint. (string)
S3_ENDPOINT=http://host.docker.internal:9000

# S3 bucket to save world data. (string)
S3_BUCKET=premises

# AWS access key for S3. (string)
AWS_ACCESS_KEY_ID=premises

# AWS secret access key for S3. (string)
AWS_SECRET_ACCESS_KEY=password

# Minecraft users who have privileges. ([]string)
PREMISES_GAME_OPERATORS=

# Minecraft users who can access the game. ([]string)
PREMISES_GAME_WHITELIST=

# Secret key to encrypt session information with. (string)
PREMISES_SECRET=secret

# Allowed value for Origin HTTP header (e.g. https://example.com). (string)
PREMISES_ALLOWED_ORIGIN=http://localhost:8000

PREMISES_PROXY_API_ENDPOINT=http://host.docker.internal:8001

PREMISES_GAME_DOMAIN=localhost

PREMISES_REDIS_ADDRESS=redis:6379

PREMISES_REDIS_PASSWORD=

PREMISES_POSTGRES_ADDRESS=postgres:5432

PREMISES_POSTGRES_USER=premises

PREMISES_POSTGRES_PASSWORD=password

PREMISES_POSTGRES_DB=premises
