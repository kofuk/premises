# -*- shell-script -*-

# Run web server debug mode. (bool)
premises_debug_web='false'

# ConoHa username. (string)
premises_conoha_username='user'

# ConoHa access token (not real password to login to ConoHa's control panel). (string)
premises_conoha_password='password'

# ConoHa tenant ID. (string)
premises_conoha_tenantId='tenantId'

# ConoHa endpoint for  Identity service. (string)
premises_conoha_services_identity='http://host.docker.internal:8010/identity/v3'

# ConoHa endpoint for Compute service. (string)
premises_conoha_services_compute='http://host.docker.internal:8010/compute/v2.1'

# ConoHa endpoint for Network service. (string)
premises_conoha_services_network='http://host.docker.internal:8010/network'

# ConoHa endpoint for Volume service. (string)
premises_conoha_services_volume='http://host.docker.internal:8010/volume/v3'

# Name tag used for premises-related resources
premises_conoha_nameTag='mc-premises'

# S3 endpoint. (string)
premises_s3_endpoint='http://host.docker.internal:9000'

# S3 bucket to save world data. (string)
premises_s3_bucket='premises'

# AWS access key for S3. (string)
premises_aws_accessKey='premises'

# AWS secret access key for S3. (string)
premises_aws_secretKey='password'

# Motd of the game.
premises_game_motd=''

# Minecraft users who have privileges. ([]string)
premises_game_operators=''

# Minecraft users who can access the game. ([]string)
premises_game_whitelist=''

# Secret key to encrypt session information with. (string)
premises_controlPanel_secret='secret'

# Allowed value for Origin HTTP header (e.g. https://example.com). (string)
premises_controlPanel_origin='http://localhost:8000'

premises_controlPanel_proxyApi='http://host.docker.internal:8001'

premises_controlPanel_gameDomain='localhost'

premises_controlPanel_redis_address='redis:6379'

premises_controlPanel_redis_password=''

premises_controlPanel_postgres_address='postgres'

premises_controlPanel_postgres_port='5432'

premises_controlPanel_postgres_user='premises'

premises_controlPanel_postgres_password='password'

premises_controlPanel_postgres_dbName='premises'
